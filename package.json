{
  "name": "unplugin-auto-import-styles",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.10.0",
  "description": "Auto import css/sass/less for Vite",
  "author": "Qinghuan <qinghuanhu@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Asaki-M/unplugin-auto-import-styles#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asaki-M/unplugin-auto-import-styles.git"
  },
  "bugs": {
    "url": "https://github.com/Asaki-M/unplugin-auto-import-styles/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "auto-import"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean",
    "dev": "tsup src/*.ts --watch src",
    "typecheck": "tsc"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.6.3",
    "tsup": "^8.2.4",
    "vite": "^5.4.4"
  }
}